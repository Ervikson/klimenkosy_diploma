server.port: 5601
server.host: "0.0.0.0"

# Адрес Elasticsearch, к которому подключается Kibana
elasticsearch.hosts: ["http://192.168.1.4:9200"]

# (Опционально) имя пользователя и пароль, если включена аутентификация
# elasticsearch.username: "{{ elasticsearch_user | default('') }}"
# elasticsearch.password: "{{ elasticsearch_password | default('') }}"

# Логи Kibana
logging.dest: /var/log/kibana/kibana.log
logging.verbose: false